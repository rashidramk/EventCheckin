          .clr
        .image_box
          %img{:alt => "image", :border => "0", :src => "/assets/image_calendar.png"}/
        .clr
      .all_footer
        .footer
          %div
            %img{:alt => "image", :border => "0", :src => "/assets/BORDER_TOP.png"}/
