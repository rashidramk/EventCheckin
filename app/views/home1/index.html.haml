          .fl
            .home_image_box2
              %div
                %a{:href => "#."}
                  %img{:alt => "image", :border => "0", :src => "/assets/user_image6.png"}/
              .home_image_text2 News
            .home_image_box2
              %div
                %a{:href => "#."}
                  %img{:alt => "image", :border => "0", :src => "/assets/user_image7.png"}/
